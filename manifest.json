{
  "manifest_version": 3,
  "name": "lowKey",
  "version": "1.0",
  "description": "lowKey transform website text to lowercase, uppercase.",

  "permissions": ["activeTab", "storage"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "lowKey"
  },

  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
